{
  "name": "new-folder",
  "version": "1.0.0",
  "description": "Node.js first app",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --exec babel-node ./index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:sync": "babel-node ./db/sync.js",
    "db:seed": "sequelize db:seed:all",
    "db:restart": "sequelize db:drop && sequelize db:create"
  },
  "keywords": [
    "Node.js",
    "javascript"
  ],
  "author": "Nurlan Mirzayev",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "express-graphql": "^0.8.0",
    "graphql": "^14.2.1",
    "graphql-tools": "^4.0.4",
    "jsonwebtoken": "^8.5.0",
    "merge-graphql-schemas": "^1.5.8",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.19",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.43.0",
    "sequelize-cli": "^5.4.0",
    "through2": "^3.0.0"
  }
}
